{
  "manifest_version": 3,
  "name": "Gov CSS Modifier",
  "version": "1.0",
  "description": "Modify the CSS for Indian government websites.",
  "host_permissions": ["https://*.gov.in/*"],
  "content_scripts": [
    {
      "matches": ["https://*.gov.in/*"],
      "css": ["style.css"],
      "js": ["content_script.js"]
    }
  ]
}